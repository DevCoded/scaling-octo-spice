//Librerias

# include <stdio.h>

//main

main(){

   int num1=10;  //Declaración de variables
   int num2=30;
   int res=0;
   
   //Operación
   res = num1 + num2;
   
   //Impresión
   printf( "\n\n\t El resultado de %i + %i = %i", num1, num2, res);
   
   
   }
   
   
